{
  "AppPaths": {
    // relative to appRoot
    "swaggerFile": "/api/swagger.yaml",
    "controllersDir": "/src/controllers",
    "mockControllersDir": "/src/mocks",
    "helpers": "/src/helpers",
    "controllers": "/src/controllers"
  },
  "publicPath": [
    "/api/user/register",
    "/api/auth",
    "/api-docs"
    ],
  "fileRepository": {
    "privatePath": "./ressources/images/",
    "publicPath": "{host}/ressources/images/"
  },
  "server": {
    "mongo": {
      "connectionString": "mongodb://nanslaupretre.fr/ynovDockerDB",
      "options": {
        "server": {
          "poolSize": 10
        }
      }
    },
    "host": "nanslaupretre.fr",
    "port": 10010,
    "frontUrl": [
      "http://localhost:4200",
      "http://nanslaupretre.fr:4200"
    ],
    "auth": {
      "tokenDuration": {
        "unit": "hours",
        "value": 5
      }
    }
  }
}